{
  "name": "swagger-code-gen-liquid",
  "version": "1.0.15",
  "main": "./dist/index",
  "typings": "./dist/",
  "scripts": {
    "test": "node ./test/swagger.parser.test.js",
    "start": "tsc -p ./src",
    "build": "npm run clean && tsc -p ./src && npm run copy-files",
    "publish": "npm publish",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u 1 src/**/*.liquid dist/"
  },
  "bin": {
    "scg": "./dist/index.js"
  },
  "keywords": [
    "template",
    "liquid",
    "swagger",
    "typescript",
    "generator",
    "codegen"
  ],
  "author": "yuzhigang",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yuzhigang/swagger-code-generator.git"
  },
  "bugs": {
    "url": "https://github.com/yuzhigang/swagger-code-generator/issues"
  },
  "homepage": "https://github.com/yuzhigang/swagger-code-generator#readme",
  "description": "swagger code generator with self-definition templates",
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.0.2",
    "camelcase": "^6.2.0",
    "liquidjs": "^9.28.0",
    "prettier": "^2.2.1",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@types/node": "^12.7.2",
    "@types/prettier": "^1.19.1",
    "@types/yargs": "^15.0.12",
    "copyfiles": "^2.4.1",
    "openapi-types": "^7.2.2",
    "rimraf": "^3.0.2",
    "tslint": "^5.10.0",
    "typescript": "^4.0.2"
  }
}